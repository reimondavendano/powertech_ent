{
  "name": "angular-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href=/powertech_ent/",
    "start:dev": "nest start --watch",
    "deploy-ghpages": "npm run build && npm run copy-for-ghpages",
    "copy-for-ghpages": "powershell -Command \"If (Test-Path -Path 'docs/browser') { Get-ChildItem -Path 'docs/browser/*' -Recurse -Force | Move-Item -Destination 'docs/' -Force -ErrorAction SilentlyContinue; Remove-Item -Path 'docs/browser' -Recurse -Force -ErrorAction SilentlyContinue }; Copy-Item -Path 'docs/index.html' -Destination 'docs/404.html' -Force -ErrorAction SilentlyContinue\""
  },
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@emailjs/browser": "^4.4.1",
    "@nestjs/config": "^4.0.2",
    "@supabase/supabase-js": "^2.52.1",
    "lucide-angular": "^0.525.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@nestjs/cli": "^11.0.7",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2"
  }
}
